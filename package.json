{
  "name": "ll",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "server": "npm run start:dev --prefix ll_server",
    "server-install": "npm install --prefix ll_server",
    "client": "npm run dev --prefix ll_client",
    "client-install": "npm install --prefix ll_client",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose up --build -d",
    "docker:logs": "docker-compose logs -f",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:setup-db": "./scripts/setup-db.sh",
    "install:all": "pnpm install && pnpm run client-install && pnpm run server-install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.17.0",
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
